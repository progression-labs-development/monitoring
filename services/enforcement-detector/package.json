{
  "name": "enforcement-detector",
  "version": "0.1.0",
  "private": true,
  "description": "Scheduled detector that enumerates live cloud resources and creates incidents for rogue resources",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.700.0",
    "@aws-sdk/client-ecs": "^3.700.0",
    "@aws-sdk/client-elasticache": "^3.700.0",
    "@aws-sdk/client-iam": "^3.700.0",
    "@aws-sdk/client-lambda": "^3.700.0",
    "@aws-sdk/client-rds": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "@google-cloud/compute": "^4.0.0",
    "@google-cloud/functions": "^3.0.0",
    "@google-cloud/redis": "^4.0.0",
    "@google-cloud/run": "^1.0.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/storage": "^7.0.0",
    "@hono/node-server": "^1.13.7",
    "hono": "^4.6.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
